{{> header}}
<div class="page">
    <div class="weui-msg">
        <div class="weui-msg__icon-area"><i class="weui-icon-success weui-icon_msg"></i></div>
        <div class="weui-msg__text-area">
            <h2 class="weui-msg__title">{{page.msg_title}}</h2>
            <p class="weui-msg__desc">{{page.msg_desc}}</p>
        </div>
        <div class="weui-msg__opr-area">
            <p class="weui-btn-area">
                <a href="{{page.close_link}}" class="weui-btn weui-btn_primary">{{page.btn_title}}</a>
            </p>
        </div>
        <div class="weui-msg__extra-area">
            <div class="weui-footer">
                <p class="weui-footer__links">
                    <a href="{{page.close_link}}" class="weui-footer__link">{{page.footer_title}}</a>
                </p>
                <p class="weui-footer__text">Copyright &copy; 2016-2017 9jialu</p>
            </div>
        </div>
    </div>
</div>
{{> footer}}